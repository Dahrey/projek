			function empty(e) {
		      switch (e) {
		        case "":
		        case 0:
		        case "0":
		        case null:
		        case false:
		        case typeof(e) == "undefined":
		          return true;
		        default:
		          return false;
		      }
		    }
		    function show_xpanel(class_){
		      $('.'+class_).show();
		    }
		    function hide_xpanel(class_){
		      $('.'+class_).hide();
		    }
